rError\r\n                ?\r\n                <div className=\"login__error\">Error: {errorMessage} </div>\r\n                :\r\n                <></>\r\n            }\r\n            <div className=\"login__bottom\">\r\n                <p>*No need to use your actual email address. Treat it as a username to login to this specific application</p>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Login",["84","85"],"C:\\Users\\placi\\Desktop\\hp-development\\react\\note-taker\\src\\StateProvider.js",["86"],"C:\\Users\\placi\\Desktop\\hp-development\\react\\note-taker\\src\\Scroller.js",[],"C:\\Users\\placi\\Desktop\\hp-development\\react\\note-taker\\src\\NewSection.js",["87","88","89","90"],"C:\\Users\\placi\\Desktop\\hp-development\\react\\note-taker\\src\\entries.js",[],["91","92"],"C:\\Users\\placi\\Desktop\\hp-development\\react\\note-taker\\src\\index.js",["93"],"C:\\Users\\placi\\Desktop\\hp-development\\react\\note-taker\\src\\Entry.js",[],"C:\\Users\\placi\\Desktop\\hp-development\\react\\note-taker\\src\\firebase.js",[],{"ruleId":"94","severity":1,"message":"95","line":8,"column":22,"nodeType":"96","messageId":"97","endLine":8,"endColumn":33},{"ruleId":"94","severity":1,"message":"98","line":29,"column":17,"nodeType":"96","messageId":"97","endLine":29,"endColumn":22},{"ruleId":"94","severity":1,"message":"99","line":30,"column":17,"nodeType":"96","messageId":"97","endLine":30,"endColumn":22},{"ruleId":"94","severity":1,"message":"100","line":5,"column":8,"nodeType":"96","messageId":"97","endLine":5,"endColumn":13},{"ruleId":"94","severity":1,"message":"95","line":25,"column":20,"nodeType":"96","messageId":"97","endLine":25,"endColumn":31},{"ruleId":"94","severity":1,"message":"101","line":26,"column":10,"nodeType":"96","messageId":"97","endLine":26,"endColumn":17},{"ruleId":"94","severity":1,"message":"102","line":26,"column":19,"nodeType":"96","messageId":"97","endLine":26,"endColumn":29},{"ruleId":"94","severity":1,"message":"10